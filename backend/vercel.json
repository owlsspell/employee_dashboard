{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "headers": [
    {
      "source": "https://employee-dashboard-neon.vercel.app/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": [
          "https://employee-dashboard-backend.vercel.app/",
          "https://employee-dashboard-backend-owlsspell.vercel.app/",
          "https://employee-dashboard-backend-git-main-owlsspell.vercel.app/"
        ],
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Methods": "GET,HEAD,OPTIONS,POST,PUT",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization"
      }
    },
    {
      "source": "https://employee-dashboard-frontend-owlsspell.vercel.app/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": [
          "https://employee-dashboard-backend.vercel.app/",
          "https://employee-dashboard-backend-owlsspell.vercel.app/",
          "https://employee-dashboard-backend-git-main-owlsspell.vercel.app/"
        ],
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Methods": "GET,HEAD,OPTIONS,POST,PUT",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization"
      }
    },
    {
      "source": "https://employee-dashboard-frontend-git-main-owlsspell.vercel.app/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": [
          "https://employee-dashboard-backend.vercel.app/",
          "https://employee-dashboard-backend-owlsspell.vercel.app/",
          "https://employee-dashboard-backend-git-main-owlsspell.vercel.app/"
        ],
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Methods": "GET,HEAD,OPTIONS,POST,PUT",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization"
      }
    }
  ]
}
